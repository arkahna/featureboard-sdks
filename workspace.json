{
  "version": 2,
  "projects": {
    "js-sdk": {
      "root": "libs/js-sdk",
      "projectType": "library",
      "sourceRoot": "libs/js-sdk/src",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/js-sdk/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/js-sdk/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/libs/js-sdk"]
        },
        "package": {
          "executor": "@wanews/nx-typescript-project-references:package",
          "options": {
            "main": "libs/js-sdk/src/index.ts",
            "tsConfig": "libs/js-sdk/tsconfig.json"
          }
        }
      }
    },
    "node-sdk": {
      "root": "libs/node-sdk",
      "projectType": "library",
      "sourceRoot": "libs/node-sdk/src",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/node-sdk/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/node-sdk/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/libs/node-sdk"]
        },
        "package": {
          "executor": "@wanews/nx-typescript-project-references:package",
          "options": {
            "main": "libs/node-sdk/src/index.ts",
            "tsConfig": "libs/node-sdk/tsconfig.json"
          }
        }
      }
    },
    "react-sdk": {
      "root": "libs/react-sdk",
      "projectType": "library",
      "sourceRoot": "libs/react-sdk/src",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/react-sdk/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/react-sdk/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/libs/react-sdk"]
        },
        "package": {
          "executor": "@wanews/nx-typescript-project-references:package",
          "options": {
            "main": "libs/react-sdk/src/index.ts",
            "tsConfig": "libs/react-sdk/tsconfig.json"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  }
}
